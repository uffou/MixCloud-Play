{
	"name": "mixcloud-play",
	"version": "0.10.2",
	"description": "The Mixcloud desktop client",
	"keywords": [
		"music",
		"osx",
		"macOS",
		"mixcloud",
		"wrapper",
		"media keys",
		"electron"
	],
	"author": {
		"name": "Codemotion Ltd."
	},
	"contributors": [
		{
			"name": "Mountain/\\Ash"
		},
		{
			"name": "Mihail Melnic"
		}
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:uffou/MixCloud-Play.git"
	},
	"homepage": "https://github.com/uffou/MixCloud-Play/issues",
	"bugs": {
		"url": "https://github.com/uffou/MixCloud-Play/issues"
	},
	"license": "UNLICENSED",
	"scripts": {
		"dev": "electron-webpack dev",
		"compile": "electron-webpack",
		"compile:debug": "ELECTRON_WEBPACK_APP_DEBUG=true electron-webpack",
		"compile:local": "ELECTRON_WEBPACK_APP_LOCAL=true yarn compile:debug",
		"build": "electron-builder --mac",
		"dist": "yarn compile && yarn build",
		"dist:debug": "yarn compile:debug && yarn build",
		"dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
	},
	"electronWebpack": {
		"title": false,
		"staticSourceDirectory": "static",
		"main": {
			"extraEntries": [
				"@/preload.js"
			]
		},
		"renderer": {
			"template": "src/renderer/index.html"
		}
	},
	"build": {
		"files": [
			"static"
		],
		"appId": "com.codemotionapps.mixcloud",
		"copyright": "Copyright Â© 2019 Codemotion Ltd.",
		"productName": "Mixcloud Play",
		"mac": {
			"target": "tar.gz",
			"category": "public.app-category.music",
			"icon": "Mixcloud.icns"
		}
	},
	"main": "dist/main/main.js",
	"devDependencies": {
		"@babel/core": "7.8.7",
		"@babel/plugin-proposal-class-properties": "7.8.3",
		"@babel/preset-env": "7.8.7",
		"electron": "3.1.13",
		"electron-builder": "22.4.0",
		"electron-webpack": "2.7.4",
		"webpack": "4.42.0"
	},
	"resolutions": {
		"webpack-sources": "1.4.3"
	},
	"dependencies": {
		"source-map-support": "0.5.16"
	},
	"renovate": {
		"extends": [
			"config:base"
		]
	}
}
